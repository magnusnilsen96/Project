FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (84.7 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (84.7 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    lowerWall
    {
        type            symmetryPlane;
    }
    upperWall
    {
        type            symmetryPlane;
    }
    pipe
    {
        type            noSlip;
    }
    defaultFaces
    {
        type            empty;
    }
}


